{
  "name": "subapp-web",
  "version": "1.0.34",
  "description": "Electrode subapp web support",
  "module": "dist/dev/index.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.browser.json && tsc -p tsconfig.node.json && tsc -p tsconfig.lib.json && clap minify -s=dist/dev",
    "test": "BABEL_ENV=src-minify xrun xarc/test-only",
    "coverage": "BABEL_ENV=src-minify xrun xarc/test-cov",
    "prepublishOnly": "xrun [[build, docs], xarc/check]",
    "docs": "xrun xarc/docs"
  },
  "keywords": [
    "web",
    "react",
    "subapp"
  ],
  "author": "Electrode (http://www.electrode.io/)",
  "contributors": [
    "Joel Chen <xchen@walmartlabs.com>"
  ],
  "license": "Apache-2.0",
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "history": "^4.9.0",
    "jsesc": "^2.5.2",
    "loadjs": "^4.2.0",
    "lodash": "^4.17.15",
    "optional-require": "^1.0.0",
    "request": "^2.88.0",
    "subapp-util": "^1.1.1",
    "xaa": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.7.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jsdom": "^16.2.3",
    "@types/lodash": "4.14.160",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.7.6",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@xarc/module-dev": "^2.2.4",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electrode-archetype-njs-module-dev": "^3.0.0",
    "electrode-react-webapp": "^3.8.9",
    "jsdom": "^15.2.1",
    "mocha": "^7.1.0",
    "mock-require": "^1.3.0",
    "nyc": "^15.0.0",
    "run-verify": "^1.2.2",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "subapp-pkg-util": "../subapp-pkg-util",
    "ts-node": "^8.6.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "fyn": {
    "dependencies": {
      "subapp-util": "../subapp-util"
    }
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "browser",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "lib",
      "node-dist",
      "test",
      "xrun*.js",
      "xrun*.ts"
    ],
    "check-coverage": true,
    "statements": 0,
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "cache": true,
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "@xarc/module-dev": {
    "features": [
      "mocha",
      "typedoc",
      "typescript"
    ]
  }
}
