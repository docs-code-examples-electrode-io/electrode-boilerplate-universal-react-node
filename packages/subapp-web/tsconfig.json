{
  "compilerOptions": {
    "esModuleInterop": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "removeComments": true,
    "strictFunctionTypes": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
